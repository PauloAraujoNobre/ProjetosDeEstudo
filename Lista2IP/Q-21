#include <stdio.h>
int main(){	
    int count, i;
    printf("Digite o valor do saque: ");
    scanf("%i", &i);
    for(;i>=100;count++){
        i = i-100;
    }
    for(;i>=50;count++){
        i = i-50;
    }
    for(;i>=20;count++){
        i = i-20;
    }
    for(;i>=10;count++){
        i = i-10;
    }
    for(;i>=5;count++){
        i = i-5;
    }
    for(;i>=2;count++){
        i = i-2;
    }
    for(;i>=1;count++){
        i = i-1;
    }
    printf("SerÃ£o utilizadas %d notas", count);
	return 0;
}
