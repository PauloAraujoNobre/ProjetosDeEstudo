#include <stdio.h>
int main(){
    int i, soma = 0, count = 0;
    int x[100];

    for(i=0;i<100;i++){
        x[i] = i;
    }
    for(i=0;count<50;i++){
        if(x[i]%2 == 0){
            soma = soma + x[i];
            printf("%d\n", x[i]);
            count++;
        }
    }
    printf("Soma: %d\n", soma);
    return 0;
}
